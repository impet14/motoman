<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
<xacro:macro name="motoman_gp25" params="prefix">

    <!-- Properties -->
    <material name="yaskawa_blue">
          <color rgba="0.12941 0.14902 0.74902 1" />
    </material>

    <!-- link list -->
    <link name="${prefix}base_link">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp25_support/meshes/visual/gp25_base_link.stl"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp25_support/meshes/collision/gp25_base_link.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_1_s">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp25_support/meshes/visual/gp25_link_1_s.stl"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp25_support/meshes/collision/gp25_link_1_s.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_2_l">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp25_support/meshes/visual/gp25_link_2_l.stl"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp25_support/meshes/collision/gp25_link_2_l.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_3_u">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp25_support/meshes/visual/gp25_link_3_u.stl"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp25_support/meshes/collision/gp25_link_3_u.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_4_r">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp25_support/meshes/visual/gp25_link_4_r.stl"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp25_support/meshes/collision/gp25_link_4_r.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_5_b">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp25_support/meshes/visual/gp25_link_5_b.stl"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp25_support/meshes/collision/gp25_link_5_b.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}link_6_t">
        <visual>
            <geometry>
                <mesh filename="package://motoman_gp25_support/meshes/visual/gp25_link_6_t.stl"/>
            </geometry>
            <material name="yaskawa_blue"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://motoman_gp25_support/meshes/collision/gp25_link_6_t.stl"/>
            </geometry>
        </collision>
    </link>
    <link name="${prefix}tool0"/>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="${prefix}joint_1_s" type="revolute">
        <parent link="${prefix}base_link"/>
        <child link="${prefix}link_1_s"/>
        <origin xyz="0 0 0.505" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <limit lower="-3.1416" upper="3.1416" effort="3087" velocity="3.6651"/>
    </joint>
    <joint name="${prefix}joint_2_l" type="revolute">
        <parent link="${prefix}link_1_s"/>
        <child link="${prefix}link_2_l"/>
        <origin xyz="0.150 0 0" rpy="0 0 0" />
        <axis xyz="0 1 0" />
        <limit lower="-1.8326" upper="2.7052" effort="3087" velocity="3.6651"/>
    </joint>
    <joint name="${prefix}joint_3_u" type="revolute">
        <parent link="${prefix}link_2_l"/>
        <child link="${prefix}link_3_u"/>
        <origin xyz="0 0 0.760" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="-1.5009" upper="2.7925" effort="926.1" velocity="4.6251"/>
    </joint>
    <joint name="${prefix}joint_4_r" type="revolute">
        <parent link="${prefix}link_3_u"/>
        <child link="${prefix}link_4_r"/>
        <origin xyz="0.795 0 0.200" rpy="0 0 0" />
        <axis xyz="-1 0 0" />
        <limit lower="-3.4906" upper="3.4906" effort="88.2" velocity="7.3303"/>
    </joint>
    <joint name="${prefix}joint_5_b" type="revolute">
        <parent link="${prefix}link_4_r"/>
        <child link="${prefix}link_5_b"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="0 -1 0" />
        <limit lower="-2.6179" upper="2.6179" effort="70.56" velocity="7.3303"/>
    </joint>
    <joint name="${prefix}joint_6_t" type="revolute">
        <parent link="${prefix}link_5_b"/>
        <child link="${prefix}link_6_t"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="-1 0 0" />
        <limit lower="-7.9412" upper="7.9412" effort="50.31" velocity="15.4461"/>
    </joint>
    <joint name="${prefix}joint_6_t-tool0" type="fixed">
        <origin xyz="0.100 0 0" rpy="3.1415926 -1.570796 0"/>
        <parent link="${prefix}link_6_t"/>
        <child link="${prefix}tool0"/>
    </joint>
    <!-- end of joint list -->

    <!-- ROS base_link to Robot Manufacturer World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
        <origin xyz="0 0 0.505" rpy="0 0 0"/>
        <parent link="${prefix}base_link"/>
        <child link="${prefix}base"/>
    </joint>

    </xacro:macro>
</robot>

